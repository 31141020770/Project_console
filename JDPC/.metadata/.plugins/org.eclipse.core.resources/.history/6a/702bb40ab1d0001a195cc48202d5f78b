package test;

public class Main {

	public static void main(String[] args) {
		String[][] graph = new String[12][30];
		String [] nameGraph = new String[20];
		
		for (int i = 0; i < graph.length; i++) {
			for (int j = 0; j < 30; j++) {				
					graph[i][j] = " ";
			}
		}
		for (int i = 0; i < 30; i++) {
			graph[11][i] = "*";
		}
		for (int i = 0; i < 12; i++) {
			graph[i][0] = "*";
		}
		graph[0][0] = "^";
		for (int i = 11; i > 11 - 2 ; i++) {
			graph[i][4] = "**";
		}
		graph[0][1] = "Sum Money ";
		/* graph[9- graph_1][3] = "*"; */

		/*----------------- graph------------------------------ */

		for (int i = 0; i < 12; i++) {
			for (int j = 0; j < 30; j++) {
				System.out.print(graph[i][j]);
			}
			System.out.println();
		}
		
		for (int i = 0; i < nameGraph.length; i++) {
			nameGraph[i] = " ";
			nameGraph[3] = " M1   ";
			nameGraph[6] = "  M2   ";
			nameGraph[9] = "  M3";
			System.out.print(nameGraph[i]);
		}
		System.out.println();
		System.out.println("-------------------		--------------------");

		/*----------------- graph------------------------------ */

	}

}
