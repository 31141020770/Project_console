package test;

public class HCN {
	private double cd;
	private double cr;
	public double getCd() {
		return cd;
	}
	HCN(double cd, double cr) {
		this.cd = cd;
		this.cr = cr;
	}
	public void setCd(double cd) {
		this.cd = cd;
	}
	public double getCr() {
		return cr;
	}
	public void setCr(double cr) {
		this.cr = cr;
	}
	
	public double DT() {
		return cd * cr;
	}
}
